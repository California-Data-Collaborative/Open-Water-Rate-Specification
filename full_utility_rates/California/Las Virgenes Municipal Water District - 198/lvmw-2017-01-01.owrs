  utility_name: "Las Virgenes Municipal Water District"
  bill_frequency: monthly
rate_structure:
  RESIDENTIAL_SINGLE:
    service_charge:
      depends_on: meter_size
      values:
        3/4"  : 21.73
        1"    : 35.14
        1.5"  : 68.65
        2"    : 108.86
        3"    : 236.20
        4"    : 423.85
        6"    : 1073.94
        8"    : 1878.17
        10"   : 2816.44
    gpcd: 55
    landscape_factor: 0.8
    days_in_period: 30.4
    indoor: "gpcd*hhsize*days_in_period*(1/748)"
    outdoor: "landscape_factor*et_amount*irr_area*0.632*(1/748)"
    budget: "indoor+outdoor"
    tier_starts:
      - 0
      - indoor
      - 101%
      - 150%
    tier_prices:
      - 2.46
      - 3.24
      - 4.00
      - 5.02
    commodity_charge: Budget
    elevation_rate:
